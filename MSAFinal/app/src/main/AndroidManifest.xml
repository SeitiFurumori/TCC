<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.msa">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_msa"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MSA">
        <activity
            android:name=".EmailActivity"
            android:exported="true" />
        <activity
            android:name=".GraficoActivity"
            android:exported="true" />
        <activity
            android:name=".EsqueceuASenhaActivity"
            android:exported="true" />
        <activity
            android:name=".TelaListar"
            android:exported="true" />
        <activity
            android:name=".TrocaSenha"
            android:exported="true" />
        <activity
            android:name=".TrocaNome"
            android:exported="true" />
        <activity
            android:name=".SplashDevMedia"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PaginaInicialActivity"
            android:exported="true" />
        <activity
            android:name=".CadastrarUsuario"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>